pipeline {
  agent any
  stages {
    stage('Starting') {
      steps {
        echo 'Starting Build'
      }
    }
    stage('Build') {
      steps {
        echo 'Hello from build'
      }
    }
    stage('Test') {
      steps {
        echo 'Testing Testing 123'
      }
    }
    stage('Deploy') {
      steps {
        echo 'Deploy something'
      }
    }
  }
}